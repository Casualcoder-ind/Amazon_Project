Create table Inventory
(
inventory_id int PRIMARY KEY  ,
product_id int,
stock INT ,
warehouse_id int,
last_stock_date DATE,
CONSTRAINT inventory_fk_orders FOREIGN KEY (product_id) REFERENCES products(product_id)
);
Select * from products;
