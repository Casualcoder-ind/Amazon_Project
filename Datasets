---- Amazon Project 

-- Categary Table

***sql
create table Categary 
(
categary_id INT PRIMARY KEY
, categary_name VARCHAR(20)
);
***

Select * from categary ;

-- Customers Table

Select * from customers;
alter table customers
drop name;

alter table customers
ADD COLUMN first_name VARCHAR (20),
ADD COLUMN last_name VARCHAR(20),
ADD COLUMN state VARCHAR (6),
ADD COLUMN Address VARCHAR (20) Default ('xxxx')
;

Select * from customers;

-- Seller Table

Create table Sellers
(
seller_id INT PRIMARY KEY ,
seller_name VARCHAR (25),
origin VARCHAR (5)
);

Select * from sellers;

-- updateing data type
ALter table sellers
modify column origin VARCHAR (20)
;

