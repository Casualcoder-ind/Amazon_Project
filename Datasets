Create table Shippings
(
shipping_id INT PRIMARY KEY,
order_id INT,
shipping_date DATE,
return_date DATE ,
shipping_providers VARCHAR(20),
delivery_status VARCHAR(20),
CONSTRAINT shippings_fk_orders FOREIGN KEY (order_id) REFERENCES Orders(order_id)
);

